		{
			"name": "Get a list of booking IDs",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Ensure response is in JSON format\", () => {\r",
							"    pm.expect(pm.response).to.be.json; \r",
							"})\r",
							"\r",
							"pm.test(\"Ensure that the booking id is a number\", () => {\r",
							"    for (let i = 0; i < response.length; i++) {\r",
							"        pm.expect(response[i].bookingid).to.be.a('number');\r",
							"    }\r",
							"})\r",
							"\r",
							"console.log(\"There is \" + response.length + \" entries on this search\");"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/booking",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"booking"
					],
					"query": [
						{
							"key": "firstname",
							"value": "Sally",
							"disabled": true
						},
						{
							"key": "lastname",
							"value": "Jones",
							"disabled": true
						},
						{
							"key": "checkin",
							"value": "2023-03-25",
							"disabled": true
						},
						{
							"key": "checkout",
							"value": "2023-03-25",
							"disabled": true
						},
						{
							"key": "totalprice",
							"value": "100",
							"disabled": true
						},
						{
							"key": "depositpaid",
							"value": "false",
							"disabled": true
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Create a booking",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.collectionVariables.set(\"bookingid\", response.bookingid);\r",
							"\r",
							"pm.test(\"Booking entry is JSON\", () => {\r",
							"    const headerContentType = pm.response.headers.get(\"Content-Type\")\r",
							"    pm.expect(headerContentType).to.contain(\"application/json\");\r",
							"})\r",
							"\r",
							"pm.test(\"Booking id is a number\", () => {\r",
							"    pm.expect(response.bookingid).to.be.a(\"number\");\r",
							"})\r",
							"\r",
							"pm.test(\"Booking id is not empty\", () => {\r",
							"    pm.expect(response.bookingid).to.not.be.null;\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
